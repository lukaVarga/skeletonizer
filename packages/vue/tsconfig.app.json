{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
  ],
  "exclude": [
    "src/**/spec/*",
    "src/**/*.spec.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./vue",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
